          "deleted_at": {
            "description": "Asset deletion time.",
            "title": "Deleted At",
            "type": "string"
          },
          "id": {
            "description": "Unique ID for this asset.",
            "title": "Id",
            "type": "string"
          }
        },
        "required": [
          "id",
          "deleted_at"
        ],
        "title": "DeleteAssetResponse",
        "type": "object"
      },
      "FileData": {
        "description": "Data class for `file` asset type.",
        "properties": {
          "asset_type": {
            "const": "file",
            "default": "file",
